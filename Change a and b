using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Change_a_and_b
{
    class Program
    {
        static void Main(string[] args)
        {
            int a = 5;
            int b = 10;
            Console.WriteLine("a={0} b={1}", a, b);
            int c = a + b;
            a = b;
            b = c - a;
            Console.WriteLine("a={0} b={1}", a, b);
        }
    }
}
